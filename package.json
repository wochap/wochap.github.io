{
  "name": "wochap",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "prebuild": "shx rm -rf dist && shx mkdir -p dist || true",
    "build": "cross-env SSR=false NODE_ENV=production webpack --mode production",
    "postbuild": "SSR=true NODE_ENV=ssr webpack --mode none && cd dist && sitemap-static --ignore-file=ignore.json --prefix=https://geanmarroquin.com/ --pretty . > sitemap.xml",
    "dev": "cross-env SSR=false NODE_ENV=development webpack-dev-server --mode development",
    "dev:inspect": "cross-env SSR=false NODE_ENV=development node --inspect-brk node_modules/.bin/webpack-dev-server --mode development",
    "lint:fix": "cross-env SSR=false NODE_ENV=production eslint . --fix",
    "test": "cross-env SSR=false NODE_ENV=test jest --config=test/jest.config.json",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:coverage-travis": "npm run test -- --coverage && cat ./coverage/lcov.info | coveralls",
    "deploy": "gh-pages -d dist -b master"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "npm run test -- --bail --findRelatedTests"
    ],
    "*.json": [
      "pretty-quick --staged"
    ]
  },
