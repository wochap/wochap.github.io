{
  "name": "wochap",
  "private": true,
  "scripts": {
    "prebuild": "shx rm -rf dist && shx mkdir -p dist && shx cp -R public/* dist || true",
    "build": "cross-env NODE_ENV=production webpack",
    "postbuild": "cross-env NODE_ENV=ssr webpack && cd dist && sitemap-static --ignore-file=ignore.json --prefix=https://geanmarroquin.com/ --pretty . > sitemap.xml && http-server",
    "build:watch": "cross-env NODE_ENV=development webpack-dev-server",
    "dev": "npm run build:watch",
    "dev:inspect": "cross-env NODE_ENV=development node --inspect node_modules/.bin/webpack-dev-server",
    "test": "cross-env NODE_ENV=test jest --config=test/jest.config.json",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:coverage-travis": "npm run test -- --coverage && cat ./coverage/lcov.info | coveralls",
    "deploy": "gh-pages -d dist -b master"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "deepmerge": "^1.3.1",
    "express": "^4.14.0",
